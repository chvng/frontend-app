{
  "name": "sb1-frontend",
  "version": "0.1.0",
  "description": "Simple frontend application with SpareBank1 Frontend Framework (FFE), Webpack, ReactJS, ExpressJS and Hot Module Replacement (HMR).",
  "main": "src/backend/server.js",
  "engine": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "dev": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development --watch",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development --history-api-fallback",
    "start": "node src/backend/server.js",
    "test": ""
  },
  "dependencies": {
    "express": "4.17.3",
    "express-http-proxy": "1.2.0",
    "request": "2.88.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@sb1/ffe-buttons": "^9.0.5",
    "@sb1/ffe-buttons-react": "^12.1.13",
    "@sb1/ffe-cards": "^8.0.5",
    "@sb1/ffe-cards-react": "^3.0.9",
    "@sb1/ffe-checkbox-react": "^7.0.2",
    "@sb1/ffe-core": "^15.5.4",
    "@sb1/ffe-core-react": "^4.2.2",
    "@sb1/ffe-dropdown-react": "^5.1.2",
    "@sb1/ffe-form": "^13.0.7",
    "@sb1/ffe-form-react": "^6.0.4",
    "@sb1/ffe-grid": "^11.0.5",
    "@sb1/ffe-grid-react": "^10.1.1",
    "@sb1/ffe-header": "^12.0.5",
    "@sb1/ffe-icons-react": "^7.2.3",
    "@sb1/ffe-radio-button-react": "^5.0.16",
    "@sb1/ffe-tables": "^10.0.5",
    "@sb1/ffe-tables-react": "^6.0.8",
    "@sb1/ffe-webfonts": "^2.1.0",
    "autoprefixer": "9.4.5",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "file-loader": "^3.0.1",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "react": "^16.13.0",
    "react-css-collapse": "^3.6.0",
    "react-dom": "^16.13.0",
    "react-rectangle": "^1.3.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "style-loader": "0.23.1",
    "svg-url-loader": "^3.0.3",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
