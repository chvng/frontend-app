    "@sb1/ffe-accordion": "^5.0.7",
    "@sb1/ffe-accordion-react": "^6.3.3",
    "@sb1/ffe-account-selector-react": "^12.0.6",
    "@sb1/ffe-buttons": "^8.2.6",
    "@sb1/ffe-buttons-react": "^11.3.1",
    "@sb1/ffe-cards": "^7.0.1",
    "@sb1/ffe-cards-react": "^2.2.5",
    "@sb1/ffe-checkbox-react": "^7.0.0",
    "@sb1/ffe-core": "^14.0.5",
    "@sb1/ffe-core-react": "^3.3.0",
