{
  "name": "sb1-frontend",
  "version": "0.1.0",
  "description": "Simple frontend application with SpareBank1 Frontend Framework (FFE), Webpack, ReactJS, ExpressJS and Hot Module Replacement (HMR).",
  "main": "src/backend/server.js",
  "engine": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "dev": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development --watch",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development --history-api-fallback",
    "start": "node src/backend/server.js",
    "test": ""
  },
  "dependencies": {
